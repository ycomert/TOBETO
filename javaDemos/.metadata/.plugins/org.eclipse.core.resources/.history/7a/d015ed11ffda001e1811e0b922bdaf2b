package loopDemo;

public class Main {

	public static void main(String[] args) {
		// For loop
		for (int i = 2; i < 10; i+=2) {
			System.out.println(i);
		}
		System.out.println("For Dongusu Bitti");
		
		// While loop
		int i = 1;
		while(i<10) {
			System.out.println(i);
			i+=2;
		}
		System.out.println("While Dongusu Bitti");
		
	}

}
