package conditionals;

public class Main {

	public static void main(String[] args) {
		int sayi = 20;
		if (sayi<20) {
			System.out.println("The number is less than 20");
		}
		else if (sayi==20) {
			System.out.println("The number is equal to 20");
		}else {
			System.out.println("The number is greater than 20");
		}

	}

}
